// Staircase.cpp : This file contains the 'main' function. Program execution begins and ends there.
//

#include <iostream>
#include <string>
using namespace std;
int main()
{
	int n;
	cin >> n;
	for (int i = 1; i <= n; ++i)
	{
		std::string whs(n-i, ' ');
		std::string hashs(i, '#');
		cout << whs << hashs << endl;

	}

}
